{
    "name": "jeromejhipolito/laravel-amqp-toolkit",
    "description": "Laravel package for AMQP message handling with automatic retry, dead letter queue, and failure management",
    "keywords": ["laravel", "amqp", "rabbitmq", "queue", "retry", "dead-letter", "message-handling"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jerome Hipolito",
            "email": "jerome@betrnk-junket.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "bschmitt/laravel-amqp": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JeromeJHipolito\\AmqpToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeromeJHipolito\\AmqpToolkit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeromeJHipolito\\AmqpToolkit\\AmqpToolkitServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
